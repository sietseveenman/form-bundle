{
    "name": "wearejust/form-bundle",
    "description": "Add extra functionality to Symfony Form",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees van Egmond",
            "email": "cees.vanegmond@wearejust.com"
        },
        {
            "name": "John in 't Hout",
            "email": "john.inthout@wearejust.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wearejust\\FormBundle\\": ""
        }
    },
    "extra": { 
        "branch-alias": { 
            "dev-master": "1.x-dev" 
        }
    },
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0"
    },
    "suggest": {
        "presta/image-bundle": "Allows for cropping images, use version ^1.7",
        "vich/uploader-bundle": "Allows for saving files to entities, use version ^1.6"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
